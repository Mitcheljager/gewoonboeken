<div id="<%= dom_id listing %>">
  <p>
    <strong>Book:</strong>
    <%= listing.book_id %>
  </p>

  <p>
    <strong>Source:</strong>
    <%= listing.source_id %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= listing.price %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= listing.currency %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= listing.url %>
  </p>

  <p>
    <strong>Last scraped at:</strong>
    <%= listing.last_scraped_at %>
  </p>

</div>
