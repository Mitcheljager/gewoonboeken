<% content_for :title, "Listings" %>

<h1>Listings</h1>

<%= page_entries_info @listings %>

<hr>

<div id="listings">
  <% @listings.each do |listing| %>
    <%= render listing %>
    <p>
      <%= link_to "Show this listing", listing %>
    </p>
  <% end %>
</div>

<%= link_to "New listing", new_listing_path %>

<%= paginate @listings %>
