<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Authors:</strong>
    <% book.authors.each do |author| %>
      <span><%= author.name %></span>
    <% end %>
  </p>

  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Number of listings:</strong>
    <%= book.listings.size %>
  </p>
</div>
